<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/vegetables"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/cauliflower"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="224dp"
        android:layout_marginBottom="24dp"
        android:alpha="0.5"
        android:background="@drawable/round_button"
        app:layout_constraintBottom_toTopOf="@+id/chili"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/carrot" />

    <Button
        android:id="@+id/chili"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="173dp"
        android:layout_marginEnd="71dp"
        android:layout_marginBottom="85dp"
        android:alpha="0.5"
        android:background="@drawable/round_button"
        app:layout_constraintBottom_toTopOf="@+id/garlic"
        app:layout_constraintEnd_toStartOf="@+id/cauliflower"
        app:layout_constraintStart_toStartOf="@+id/carrot"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/brinjal"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="199dp"
        android:alpha="0.5"
        android:background="@drawable/round_button"
        app:layout_constraintBaseline_toBaselineOf="@+id/garlic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/garlic" />

    <Button
        android:id="@+id/garlic"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="77dp"
        android:layout_marginBottom="65dp"
        android:alpha="0.5"
        android:background="@drawable/round_button"
        app:layout_constraintBottom_toTopOf="@+id/peas"
        app:layout_constraintEnd_toStartOf="@+id/brinjal"
        app:layout_constraintStart_toEndOf="@+id/cucumber"
        app:layout_constraintTop_toBottomOf="@+id/chili" />

    <Button
        android:id="@+id/peas"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="95dp"
        android:layout_marginEnd="95dp"
        android:layout_marginBottom="16dp"
        android:alpha="0.5"
        android:background="@drawable/round_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/brinjal"
        app:layout_constraintStart_toStartOf="@+id/cucumber"
        app:layout_constraintTop_toBottomOf="@+id/garlic" />

    <Button
        android:id="@+id/turnip"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="189dp"
        android:layout_marginEnd="191dp"
        android:layout_marginBottom="71dp"
        android:alpha="0.5"
        android:background="@drawable/round_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/garlic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/onion" />

    <Button
        android:id="@+id/cucumber"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="292dp"
        android:layout_marginTop="95dp"
        android:layout_marginEnd="88dp"
        android:layout_marginBottom="95dp"
        android:alpha="0.5"
        android:background="@drawable/round_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/garlic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chili" />

    <Button
        android:id="@+id/carrot"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="144dp"
        android:layout_marginBottom="36dp"
        android:alpha="0.5"
        android:background="@drawable/round_button"
        app:layout_constraintBottom_toTopOf="@+id/chili"
        app:layout_constraintEnd_toStartOf="@+id/cauliflower"
        app:layout_constraintStart_toEndOf="@+id/onion" />

    <Button
        android:id="@+id/onion"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="206dp"
        android:layout_marginTop="125dp"
        android:layout_marginEnd="86dp"
        android:layout_marginBottom="167dp"
        android:alpha="0.5"
        android:background="@drawable/round_button"
        app:layout_constraintBottom_toTopOf="@+id/turnip"
        app:layout_constraintEnd_toStartOf="@+id/carrot"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>